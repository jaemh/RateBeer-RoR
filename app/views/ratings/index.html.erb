<h2>List of ratings</h2>


<table>
  <tbody>
      <ul>
        <% @ratings.each do |rating| %>
          <li> <%= rating %> <%= link_to 'delete', rating_path(rating.id), method: :delete, data: { confirm: 'Are you sure?' } %> </li>
        <% end %>
      </ul>

  <%= link_to 'new rating', new_rating_path %>    

  </tbody>
</table>


